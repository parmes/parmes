@inproceedings{Mattikalli1993,
	Abstract = {High level assembly plans prescribe the sequence in
                   which parts come together along with their motions. At
                   each stage of assembly the stability of subassemblies
                   is an important concern. In this paper we address the
                   problem of gravitational stability of assemblies of
                   frictionless rigid bodies. Solution methods to the
                   problems of determining if an assembly is stable, and
                   finding a stable orientation for a given assembly are
                   proposed. The solution methods for the two problems
                   compute constrained motions of individual parts that
                   decrease gravitational potential energy, in order to
                   determine stability and search for stable orientations.
                   Linear programming is used to obtain numerical
                   solutions. The problem of finding a stable orientation
                   is formulated as a maximin problem. The solution to
                   this problem is the first general method for
                   automatically determining stable orientations. The
                   program to solve the two problems takes as input a
                   geometric model of the assembly and information related
                   to grounding constraints. Preliminary results are
                   presented.},
	Author = {Mattikalli, Raju and Khosla, Pradeep and Repetto, Bruno and Baraff, David},
	Booktitle = {1993 International Conference on Intelligent Robots and Systems},
	Note = {Cited By: 0; Export Date: 20 July 2006; Source: Scopus},
	Pages = {652--661},
	Title = {Stability of assemblies},
	Url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0027866902&partner=40&rel=R4.5.0},
	Year = 1993,
	Bdsk-Url-1 = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0027866902&partner=40&rel=R4.5.0}}

